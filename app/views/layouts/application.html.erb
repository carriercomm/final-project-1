      </div>
      <div class="clearfix"></div>
    <div class="head">
      <div class="hlogo pull-left">
        <h1><a href="/">Stack Half Full</a></h1>
      </div>
      <div class="main-nav"></div>
    </div>

    <!-- Devise setup step 3 -->
      <!-- <p class="notice"><%= notice %></p> -->
